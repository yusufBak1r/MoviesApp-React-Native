{
    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "Debug Android",
            "type": "reactnative",
            "request": "launch",
            "platform": "android",
            "cwd": "${workspaceFolder}",
            "sourceMaps": true,
            "runArguments": ["--variant=debug"],
            "launchActivity": "${workspaceFolder}/android/app/src/main/AndroidManifest.xml"
        },
        {
            "name": "Attach to packager",
            "type": "reactnative", 
            "request": "attach",
            "cwd": "${workspaceFolder}",
            "sourceMaps": true
        }
    ]
}